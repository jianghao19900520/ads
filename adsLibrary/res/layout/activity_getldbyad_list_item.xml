<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:id="@+id/getldbyad_listview_item_rlTemplate1"
        android:layout_width="match_parent"
        android:layout_height="170dp"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp"
        android:background="#757474">
        
         <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="fitXY"
            android:src="@drawable/random_loading_loadfailed_bg"/>

        <ImageView
            android:id="@+id/getldbyad_listview_item_iv_main"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="fitXY"/>

        <ImageView
            android:id="@+id/getldbyad_listview_item_iv_bg"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/random_load_success_bg"
            android:scaleType="fitXY"
            android:visibility="gone"/>

    </RelativeLayout>

    <TextView
        android:id="@+id/getldbyad_listview_item_tv_ld"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="#333333"
        android:textSize="25sp"
        android:layout_centerInParent="true"
        android:visibility="gone"/>


</RelativeLayout>
